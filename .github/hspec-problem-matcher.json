{
  "problemMatcher": [
    {
      "owner": "hspec",
      "pattern": [
        {
          "regexp": "^[^ ]* *(.+?):(\\d+):(\\d+): ",
          "file": 1,
          "line": 2,
          "column": 3
        },
        {
          "regexp": "^  \\d+\\) (.+)",
          "message": 1
        }
      ]
    }
  ]
}
