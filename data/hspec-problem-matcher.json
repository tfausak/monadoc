{
    "//": "https://github.com/actions/toolkit/blob/7654d97/docs/problem-matchers.md",
    "problemMatcher": [
        {
            "owner": "hspec",
            "pattern": [
                {
                    "regexp": "^ *(.+):(.+):(.+): *$",
                    "file": 1,
                    "line": 2,
                    "column": 3
                },
                {
                    "regexp": "^ *(.+)\\) (.+)$",
                    "code": 1,
                    "message": 2
                }
            ]
        }
    ]
}
