{
  "dockerComposeFile": "compose.yaml",
  "extensions": [
    "EditorConfig.EditorConfig",
    "taylorfausak.purple-yolk"
  ],
  "onCreateCommand": "docker/devcontainer/on-create.sh",
  "service": "devcontainer",
  "settings": {
    "editor.formatOnSave": false,
    "editor.rulers": [
      80
    ],
    "purple-yolk.haskell.formatter.command": "false",
    "purple-yolk.haskell.interpreter.command": "cabal repl --repl-options -ddump-json",
    "purple-yolk.haskell.linter.command": "false",
    "purple-yolk.haskell.linter.onSave": false
  },
  "workspaceFolder": "/workspaces/monadoc"
}
