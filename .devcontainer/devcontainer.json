{
  "dockerComposeFile": "docker-compose.yaml",
  "extensions": [
    "EditorConfig.EditorConfig",
    "taylorfausak.purple-yolk"
  ],
  "postCreateCommand": "cabal update",
  "service": "monadoc",
  "settings": {
    "editor.formatOnSave": true,
    "purple-yolk.brittany.command": ".devcontainer/ormolu.sh --mode inplace",
    "purple-yolk.ghci.command": "cabal repl --repl-options -ddump-json",
    "purple-yolk.hlint.command": ".devcontainer/hlint.sh --json --no-exit-code",
    "purple-yolk.hlint.onSave": true
  },
  "workspaceFolder": "/workspaces/monadoc"
}
